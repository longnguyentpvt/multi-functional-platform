{
  "extends": "@shineyup/ts-config/tsconfig.next.json",
  "compilerOptions": {
    "paths": {
      "@app/components/*": [
        "./components/*"
      ],
      "@app/styles/*": [
        "./styles/*"
      ],
      "@app/utils/*": [
        "./utils/*"
      ]
    },
    "allowJs": true
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
